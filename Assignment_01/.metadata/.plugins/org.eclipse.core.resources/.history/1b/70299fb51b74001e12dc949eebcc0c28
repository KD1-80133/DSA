package assignment01_05;

public class NonRepeating {
	public static int nonRepeating(int arr[])
	{
		int n=arr.length;
		for(int i=0;i<n;i++)
		{
			int count =0;
			for(int j=0;j<n;j++)
			{
				if(arr[i]==arr[j])
				{
					break;
					}
						
				}
			}
		return -1;
	}
	

	public static void main(String[] args) {
		int[] arr = {1, 2, 3, -1, 2, 1, 0, 4, -1, 7, 8};
		int result = nonRepeating(arr);
		if(result!=-1)
		{
			System.out.println("the first non-repeating element is :"+result);
		}
		else
		{
			System.out.println("no non-repeating element found in tha array..");
		}
		

	}

}

