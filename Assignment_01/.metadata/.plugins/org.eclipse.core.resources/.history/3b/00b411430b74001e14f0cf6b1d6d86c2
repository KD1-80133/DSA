package assignment01_05;

public class NonRepeating {
	public static int nonRepeating(int arr[],int n)
	{
		int count=0;
		for(int i=0;i<n;i++)
			for(int j=0;j<n;j++)
			{
				if(i!=j && arr[i]==arr[j])
				{
					count++;
					if(count>0) {
						break;
					}
						
				}
			}
		return -1;
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

